<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
    <ValueHelpDialog title="{i18n>terminals}" ok=".onValueHelpOkPressTer" cancel=".onValueHelpCancelPressTer" supportRanges="false" key="Terminal" descriptionKey="TerminalName">
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onFilterBarSearchTer" isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="Terminal" label="{i18n>terminalID}" visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="{i18n>terminalID}" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem groupName="__$INTERNAL$" name="TerminalName" label="{i18n>terminalName}" visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="{i18n>terminalName}" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
