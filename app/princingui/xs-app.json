{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/pricingNotification-srv-api/(.*)$", 
      "target": "$1",
      "destination": "pricingNotification-srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": false,
      "scope":{
        "GET":["$XSAPPNAME.pricinganalyst"],
        "PATCH":["$XSAPPNAME.pricinganalyst"],
        "POST":["$XSAPPNAME.pricinganalyst"],
        "PUT":["$XSAPPNAME.pricinganalyst"],
        "DELETE":["$XSAPPNAME.pricinganalyst"],
        "default":["$XSAPPNAME.pricinganalyst"]
      }
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "xsuaa",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "xsuaa",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
