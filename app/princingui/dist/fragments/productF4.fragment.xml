<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
	
		title="Product"
		items="{oModel>/ProductValHelp}"
		search="_handleValueHelpSearchProduct"
		confirm="_handleValueHelpOKProduct"
		cancel="_handleValueHelpCloseProduct">
		<StandardListItem
			title="{oModel>Product}"
			description="{oModel>ProductName}" />
	</SelectDialog>
</core:FragmentDefinition>